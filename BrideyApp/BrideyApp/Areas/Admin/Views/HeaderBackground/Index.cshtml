@model List<HeaderBackground>
@{
    ViewData["Title"] = "HeaderBackground";
}
<div class="container my-5">
    <div class="table-responsive">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th class="text-center">
                        HeaderBackgrounds
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var headerBackground in Model)
                {
                    <tr>
                        <td>
                            <div class="mt-2">
                                <h6 style="color: #604b72;">@headerBackground.Key</h6>
                                @headerBackground.Value
                            </div>
                        </td>
                        <td>
                          
                                <a class="btn btn-outline-primary" asp-action="Edit" asp-route-id="@headerBackground.Id">Edit</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

